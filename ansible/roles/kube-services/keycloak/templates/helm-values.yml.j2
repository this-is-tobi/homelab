auth:
  adminUser: {{ keycloak.username }}
  adminPassword: {{ keycloak.password }}
ingress:
  enabled: true
  ingressClassName: traefik
  annotations:
    "cert-manager.io/cluster-issuer": letsencrypt-prod
  hostname: {{ keycloak.domain }}
  path: /
  tls: true
  secrets: []
production: true
proxy: edge
tls:
  enabled: false
  autoGenerated: false
postgresql:
  enabled: false
externalDatabase:
  host: pg-cluster-keycloak-rw
  port: 5432
  user: {{ keycloak.postgres.app.username }}
  database: keycloak
  password: {{ keycloak.postgres.app.password }}
