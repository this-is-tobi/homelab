---
auth:
  adminUser: {{ keycloak.username }}
  adminPassword: {{ keycloak.password }}

nodeSelector:
  node-type: worker

ingress:
  enabled: true
  ingressClassName: traefik
  annotations:
    cert-manager.io/cluster-issuer: letsencrypt-prod
  hostname: {{ keycloak.domain }}
  path: /
  tls: true
  secrets: []

production: true
proxy: edge

tls:
  enabled: false
  autoGenerated: true

global:
  storageClass: longhorn

