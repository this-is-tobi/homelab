{
  "name": "homelab",
  "env": "production",
  "provider": "self-hosted",
  "region": "fr-par",
  "prefix": {
    "name": "",
    "namespace": ""
  },
  "suffix": {
    "name": "-system",
    "namespace": "-system"
  },
  "destination": {
    "server": "https://kubernetes.default.svc"
  },
  "targetRevision": "main",
  "apps": [
    { "app": "cert-manager", "enabled": "false", "hook": "Sync", "syncWave": 0 },
    { "app": "cloudnative-pg", "enabled": "false", "hook": "Sync", "syncWave": 10 },
    { "app": "dashy", "enabled": "false", "hook": "Sync", "syncWave": 30 },
    { "app": "homelab-utils", "enabled": "false", "hook": "Sync", "syncWave":40 },
    { "app": "ingress-nginx", "enabled": "false", "hook": "Sync", "syncWave": 0 },
    { "app": "keycloak", "enabled": "false", "hook": "Sync", "syncWave": 20 },
    { "app": "kubernetes-dashboard", "enabled": "false", "hook": "Sync", "syncWave": 10 },
    { "app": "longhorn", "enabled": "true", "hook": "Sync", "syncWave": 0 },
    { "app": "prometheus-stack", "enabled": "false", "hook": "Sync", "syncWave": 10 },
    { "app": "sops", "enabled": "false", "hook": "Sync", "syncWave": 0 },
    { "app": "system-upgrade", "enabled": "false", "hook": "Sync", "syncWave": 50 },
    { "app": "teleport", "enabled": "false", "hook": "Sync", "syncWave": 10 },
    { "app": "trivy-operator", "enabled": "false", "hook": "Sync", "syncWave": 50 },
    { "app": "vault", "enabled": "false", "hook": "Sync", "syncWave": 10 },
    { "app": "vault-operator", "enabled": "true", "hook": "Sync", "syncWave": "1" },
    { "app": "vaultwarden", "enabled": "false", "hook": "Sync", "syncWave": 20 }
  ]
}
